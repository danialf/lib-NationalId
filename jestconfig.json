{
    "transform": {
        ".ts": "ts-jest"
      },
      "testEnvironment": "node",
      "testRegex": "(/__tests__/.*|\\.(test|spec))\\.ts$",
      "moduleFileExtensions": [
        "ts"
      ],
      "coveragePathIgnorePatterns": [
        "/node_modules/",
        "/test/"
      ],
      "coverageThreshold": {
        "global": {
          "branches": 90,
          "functions": 95,
          "lines": 95,
          "statements": 95
        }
      },
      "collectCoverageFrom": [
        "src/*.ts"
      ]
}